<thlogic>
    <attr sel="#showId"
          th:href="'parents/list'" th:text="'Wiadomości do rodziców'"/>
    <attr sel="#attendanceId"
          th:href="'attendance/showFormForSelectAttendance'" th:text="'Zgłoś obecność/nieobecność'"/>
    <attr sel="#attendanceListOfAllParentsId"
          th:href="'attendance/showAttendanceOfAllParents'" th:text="'Obecności/nieobecności - zestawienie wszystkich rodziców'"/>
    <attr sel="#oneDayAttendanceListOfAllParentsId"
          th:href="'attendance/showPresentDayListOfAttendanceOfAllParents'" th:text="'Pokaż obecności/nieobecności'"/>
    <attr sel="#messagesFromDirectorId"
          th:href="'messages/listOfMessagesFromDirector'" th:text="'Pokaż wiadomości od dyrekcji'"/>
    <attr sel="#questionToDirectorId"
          th:href="'questions/showFormForAddQuestion'" th:text="'Wyślij zapytanie do dyrekcji'"/>
    <attr sel="#questionListOfAllParentsId"
          th:href="'questions/showListOfQuestions'" th:text="'Zobacz zapytania od rodziców'"/>
    <attr sel="#paymentsListOfAllParentsId"
          th:href="'payments/listOfAllPayments'" th:text="'Zobacz płatności rodziców'"/>
    <attr sel="#paymentsId"
          th:href="'payments/actualPayment'" th:text="'Zobacz swoje płatności'"/>
    <attr sel="#babyStuffListId"
          th:href="'babystuff/showAllStuff'" th:text="'Zobacz zestawienie rzeczy rodziców'"/>
    <attr sel="#parentStuffId"
          th:href="'babystuff/showParentStuff'" th:text="'Zobacz zestawienie swoich rzeczy'"/>

</thlogic>