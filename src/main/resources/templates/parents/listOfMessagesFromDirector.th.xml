<thlogic>
    <attr sel="#showParentsId"
          th:each="tempMessage : ${listOfMessagesFromParents}"/>

    <attr sel="#presentDataId"
          th:text="${#temporals.format(tempMessage.date, 'dd-MM-yyyy HH:mm')}"/>
    <attr sel="#messageId"
          th:text="${tempMessage.message}"/>
    <attr sel="#footerId"
          th:replace="~{fragments/footer::footerFragment}"/>
    <attr sel="#headerId"
          th:replace="~{fragments/footer::headerFragment}"/>
</thlogic>